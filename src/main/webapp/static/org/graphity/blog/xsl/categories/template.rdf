<!DOCTYPE rdf:RDF [
    <!ENTITY blog   "http://graphity.org/blog#">
    <!ENTITY gp     "http://graphity.org/gp#">
    <!ENTITY rdf    "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <!ENTITY xsd    "http://www.w3.org/2001/XMLSchema#">
    <!ENTITY foaf   "http://xmlns.com/foaf/0.1/">
    <!ENTITY skos   "http://www.w3.org/2004/02/skos/core#">
]>
<rdf:RDF xmlns:rdf="&rdf;" xmlns:foaf="&foaf;" xmlns:skos="&skos;" xmlns:gp="&gp;">
    <rdf:Description rdf:nodeID="this">
        <rdf:type rdf:resource="&blog;CategoryDocument"/>
        <rdf:type rdf:resource="&foaf;Document"/>
        <gp:slug rdf:datatype="&xsd;string"> </gp:slug>
        <foaf:primaryTopic rdf:nodeID="thing"/>
    </rdf:Description>
    <rdf:Description rdf:nodeID="thing">
        <rdf:type rdf:resource="&blog;Category"/>
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel> </skos:prefLabel>
        <foaf:isPrimaryTopicOf rdf:nodeID="this"/>
    </rdf:Description>
</rdf:RDF>