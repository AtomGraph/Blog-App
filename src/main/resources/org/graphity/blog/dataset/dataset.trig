@prefix rdf:	<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:	<http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:	<http://www.w3.org/2001/XMLSchema#> .
@prefix ldp:	<http://www.w3.org/ns/ldp#> .
@prefix dct:	<http://purl.org/dc/terms/> .
@prefix void:	<http://rdfs.org/ns/void#> .
@prefix sioc:	<http://rdfs.org/sioc/ns#> .
@prefix foaf:	<http://xmlns.com/foaf/0.1/> .
@prefix blog:   <http://graphity.org/blog#> .

<> a blog:Site, sioc:Site, ldp:Container, foaf:Document ;
    dct:title "Blog" ;
    dct:description "Sample web application built on Graphity Client" ;
    foaf:maker <http://graphityhq.com/#company> ;
    rdfs:seeAlso <sparql>, <http://graphity.org>, <http://graphityhq.com> .

<posts> a blog:Forum, ldp:Container, sioc:Forum, sioc:Container, foaf:Document ;
    sioc:has_space <> ;
    dct:title "Posts" ;
    dct:description "A container with all blog posts" .

<categories> a blog:CategoryContainer, ldp:Container, sioc:Container, foaf:Document ;
    sioc:has_space <> ;
    dct:title "Categories" ;
    dct:description "A container with all categories" .

<sparql> a foaf:Document ;
    sioc:has_space <> ;
    dct:title "SPARQL endpoint" .