@prefix sioc:   <http://rdfs.org/sioc/ns#> .
@prefix foaf:   <http://xmlns.com/foaf/0.1/> .

<#> a owl:Ontology ;
    owl:imports gcs:, gp:, lda: , void:, foaf: ;
    rdfs:label "Sample sitemap" ;
    dct:created "2013-03-09T13:53:00+03:00"^^xsd:dateTime .

# RESOURCE CLASSES (GRAPHITY TEMPLATES)

<#ResourceContainer> a owl:Class, gp:Template ;
    rdfs:subClassOf foaf:Document, sioc:Container, ldp:Container, lda:ListEndpoint ;
    gp:uriTemplate "/resources" ;
    spin:query gcs:DescribeFromGraphWithChildren ;
    rdfs:label "Resource container" ;
    rdfs:isDefinedBy <#> .